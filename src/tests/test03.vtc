varnishtest "awsrest"

server s1 {
	rxreq
	expect req.http.e1 == "/?a=b=c"
	expect req.http.x1 == "/?a="
	expect req.http.x2 == "/?a=&b="
	expect req.http.x3 == "/?a="
	expect req.http.x4 == "/?a=&b="
	txresp
} -start

varnish v1 -vcl+backend {
	import ${vmod_awsrest};

	sub vcl_recv {
		set req.http.e1 = awsrest.form_url("/?a=b=c");
		set req.http.x1 = awsrest.form_url("/?a");
		set req.http.x2 = awsrest.form_url("/?a&b");
		set req.http.x3 = awsrest.form_url("/?a=");
		set req.http.x4 = awsrest.form_url("/?a=&b=");
	}
} -start

client c1 {
	txreq -url "/"
	rxresp
}

client c1 -run
